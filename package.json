    "uuid": "^2.0.1"
  },
  "optionalPeerDependencies": {
    "rabbitr": "^8.0.7"
  },
  "devDependencies": {
    "bluebird": "^3.3.4",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rabbitr": "^8.0.7",
    "typescript": "^1.8.7",
    "typings": "^0.7.9"
  },
  "repository": {
